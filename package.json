  "main": "expo-router/entry",
  "type": "module",
  "scripts": {
    "dev": "IP=$(ipconfig getifaddr en11 || ipconfig getifaddr en0) && sed -i '' \"s|^EXPO_PUBLIC_API_URL=.*|EXPO_PUBLIC_API_URL=http://$IP:1234|\" .env && echo \"Set EXPO_PUBLIC_API_URL to http://$IP:1234\" && expo start --clear",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "deploy": "npx expo export -p web && npx eas-cli@latest deploy",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
