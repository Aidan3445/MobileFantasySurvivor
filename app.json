{
  "scheme": "acme",
  "userInterfaceStyle": "automatic",
  "orientation": "default",
  "web": { "output": "static" },
  "plugins": [["expo-router", { "origin": "https://n" }]],
  "name": "mobile-fantasy-survivor",
  "slug": "mobile-fantasy-survivor",
  "extra": {
    "router": { "origin": "https://n" },
    "eas": { "projectId": "86931bb6-a035-48fe-8856-ff9c5684bc8e" }
  },
  "owner": "aidan3445",
  "expo": {
    "ios": {
      "bundleIdentifier": "com.aidan3445.mobilefantasysurvivor",
      "infoPlist": { "ITSAppUsesNonExemptEncryption": false },
      "requiresFullScreen": true
    },
    "android": {
      "package": "com.aidan3445.mobilefantasysurvivor",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
    },
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "PORTRAIT_UP"
        }
      ]
    ],
    "version": "1.0.0",
  }
}
