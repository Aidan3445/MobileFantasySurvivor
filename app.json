{
  "expo": {
    "scheme": "trialbyfire",
    "userInterfaceStyle": "automatic",
    "orientation": "portrait",
    "name": "Trial by Fire",
    "slug": "mobile-fantasy-survivor",
    "version": "1.0.0",
    "owner": "aidan3445",
    "icon": "./src/assets/AppIcon.png",

    "web": {
      "output": "static"
    },

    "splash": {
      "image": "./src/assets/LogoDisc.png",
      "resizeMode": "contain",
      "backgroundColor": "#E5BC8F"
    },

    "cli": {
      "appVersionSource": "remote"
    },

    "build": {
      "development": {
        "autoIncrement": true
      },
      "preview": {
        "autoIncrement": true
      },
      "production": {
        "autoIncrement": true
      }
    },

    "plugins": [
      ["expo-router", { "origin": "https://trialbyfiresurvivor.com" }],
      ["expo-screen-orientation", { "initialOrientation": "PORTRAIT_UP" }],
      [
        "expo-notifications",
        {
          "icon": "./src/assets/AppIcon.png",
          "color": "#E5BC8F"
        }
      ]
    ],

    "extra": {
      "router": {
        "origin": "https://trialbyfiresurvivor.com"
      },
      "eas": {
        "projectId": "86931bb6-a035-48fe-8856-ff9c5684bc8e"
      }
    },

    "ios": {
      "bundleIdentifier": "com.aidan3445.mobilefantasysurvivor",
      "requiresFullScreen": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "associatedDomains": [
        "applinks:trialbyfiresurvivor.com",
        "applinks:www.trialbyfiresurvivor.com"
      ]
    },

    "android": {
      "package": "com.aidan3445.mobilefantasysurvivor",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/AppIcon.png",
        "backgroundColor": "#FFFFFF"
      },
      "softwareKeyboardLayoutMode": "adjustPan",
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "trialbyfiresurvivor.com",
              "pathPrefix": "/i/"
            },
            {
              "scheme": "https",
              "host": "trialbyfiresurvivor.com",
              "pathPrefix": "/leagues/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    }
  }
}
