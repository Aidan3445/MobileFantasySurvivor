{
  "expo": {
    "scheme": "yourfantasysurvivor",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "name": "Your Fantasy Survivor",
    "slug": "mobile-fantasy-survivor",
    "version": "1.0.0",
    "owner": "aidan3445",
    "web": { "output": "static" },
    "plugins": [
      ["expo-router", { "origin": "https://yourfantasysurvivor.com" }],
      ["expo-screen-orientation", { "initialOrientation": "PORTRAIT_UP" }]
    ],
    "extra": {
      "router": { "origin": "https://yourfantasysurvivor.com" },
      "eas": { "projectId": "86931bb6-a035-48fe-8856-ff9c5684bc8e" }
    },
    "ios": {
      "bundleIdentifier": "com.aidan3445.mobilefantasysurvivor",
      "infoPlist": { "ITSAppUsesNonExemptEncryption": false },
      "requiresFullScreen": true,
      "associatedDomains": [
        "applinks:yourfantasysurvivor.com",
        "applinks:www.yourfantasysurvivor.com"
      ]
    },
    "android": {
      "package": "com.aidan3445.mobilefantasysurvivor",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "yourfantasysurvivor.com",
              "pathPrefix": "/i/"
            },
            {
              "scheme": "https",
              "host": "yourfantasysurvivor.com",
              "pathPrefix": "/leagues/join/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    }
  }
}
